Prelim:

	DIALOG.VIEW &prelim_dialog_file
	STOP
	&POINTER_ADDRESS_HEAD=DIALOG.STRING(ADDR)
	&temp=DIALOG.BOOLEAN(CHOICE.READ)
	dialog.end

	IF &temp
		(
		GOTO Read
		)
	ELSE 
