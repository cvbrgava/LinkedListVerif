NextWrite:
	
	
	&POINTER_ADDRESS_CRNT=DIALOG.STRING(POINTER_ADDRESS.NEXT)
	GOSUB SetParams
	Dialog.end
	if &REDRAW==0.
	(	
		&LINKNO=&LINKNO+1.
		PRINT "Link No: &LINKNO"
		PRINT "Validity of Values confirmed .... "
		PRINT "Generating words..."
		GOSUB WordGen
		PRINT "Writing to Memory"
		;GOSUB MemWrite
		PRINT "Done!!!"
		&ERROR=0.
	)
	ELSE
	(
		&ERROR=1.
		PRINT "&REDRAW Invalid fields found...."
		PRINT "FIx them before writing to Memory"
	)
	
	IF &POINTER_ADDRESS_NEXT!=0.
	(
		GOTO Write
	)
	ELSE IF &POINTER_ADDRESS_NEXT==0x0
	(
		Print "Last Link Detected"
		GOTO Prelim
	)
	
	
SetParams:
	IF DIALOG.BOOLEAN(Start.0)
	(
		&Start=0x0
	)
	ELSE IF DIALOG.BOOLEAN(Start.1)
	(
		&Start=0x1
	)
	ELSE 
	(
		PRINT "Invalid Entry : START"
		&REDRAW=&REDRAW+1.
	)
	
	IF DIALOG.BOOLEAN(Dir.0)
	(
		&Dir=0x0
		PRINT "&Dir"
	)
	ELSE IF DIALOG.BOOLEAN(Dir.1)
	(
		&Dir=0x1
	)
	ELSE 
	(
		PRINT "Invalid Entry : XFER_DIR"
		&REDRAW=&REDRAW+1.
	)
	
	IF DIALOG.BOOLEAN(LMEM.0)
	(
		&LMEM=0x0
	)
	ELSE IF DIALOG.BOOLEAN(LMEM.1)
	(
		&LMEM=0x1
	)
	ELSE 
	(
		PRINT "Invalid Entry : LMEM_INDEX_UPDATE"
		&REDRAW=&REDRAW+1.
	)
	
	IF DIALOG.BOOLEAN(MEMP.0)
	(
		&MEMP=0x0
	)
	ELSE IF DIALOG.BOOLEAN(MEMP.1)
	(
		&MEMP=0x1
	)
	ELSE 
	(
		PRINT "Invalid Entry : MEMP_VADDR_UPDATE"
		&REDRAW=&REDRAW+1.
	)
	
	IF DIALOG.BOOLEAN(Int.0)
	(
		&Int=0x0
	)
	ELSE IF DIALOG.BOOLEAN(Int.1)
	(
		&Int=0x1
	)
	ELSE 
	(
		PRINT "Invalid Entry : Int"
		&REDRAW=&REDRAW+1.
	)
	
	IF DIALOG.BOOLEAN(IntNo.0)
	(
		&IntNo=0x0
	)
	ELSE IF DIALOG.BOOLEAN(IntNo.1)
	(
		&IntNo=0x1
	)
	ELSE IF DIALOG.BOOLEAN(IntNo.2)
	(
		&IntNo=0x2
	)
	ELSE IF DIALOG.BOOLEAN(IntNo.3)
	(
		&IntNo=0x3
	)
	ELSE IF DIALOG.BOOLEAN(IntNo.4)
	(
		&IntNo=0x4
	)
	ELSE IF DIALOG.BOOLEAN(IntNo.5)
	(
		&IntNo=0x5
	)
	ELSE IF DIALOG.BOOLEAN(IntNo.6)
	(
		&IntNo=0x6
	)
	ELSE IF DIALOG.BOOLEAN(IntNo.7)
	(
		&IntNo=0x7
	)
	ELSE 
	(
		PRINT "Invalid Entry : IntNo"
		&REDRAW=&REDRAW+1.
	)
	
	GOSUB DialogRead NumSmpl
	ENTRY &NumSmpl
	
	GOSUB DialogRead Format.MEMP
	ENTRY &Format_MEMP
	
	GOSUB DialogRead  Format.LMEM
	ENTRY &Format_LMEM
	
	GOSUB DialogRead  Format.Mod
